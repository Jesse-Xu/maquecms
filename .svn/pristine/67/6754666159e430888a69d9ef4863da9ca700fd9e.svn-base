<?php

namespace app\admin\validate;
use think\Validate;

class BaseValidate extends Validate{
   
   #删除#
   public static function Del($data){

   		if(empty($data)){
   			return $data."不能为空~";
   		}
   		
   }
   #必填#
   public static function Required($data){

   		if(empty($data)){
   			return "条件不能为空~";
   		}
         
   		
   }

   


}